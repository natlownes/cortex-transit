class EditorialFeed
  constructor: ->
    @images = []
    for feed in [health, entertainment, sports]
      xmlDoc = $.parseXML(feed)
      xml = $(xmlDoc)
      items = xml.find('item')
      for item in items
        item = $(item)
        img = item.find('media\\:content, content')
        url = img.attr('url')
        @images.push url

  getRandom: ->
    @images[Math.floor(Math.random() * @images.length)]

module.exports = EditorialFeed
